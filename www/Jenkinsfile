pipeline {
  agent any
  stages {
        stage("sprawdz_polecenia_systemu") {
            steps {
                bat 'PATH'
                bat 'dir'
            }
        }
        stage("Wejscie do podkatalogu www i uruchomienie PHP") {
            steps {
                bat 'cd www && dir && php index.php'
                bat 'dir'
            }
        }
    }
}